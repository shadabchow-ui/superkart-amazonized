{
  "components/halothemes/header/header-mobile": "<div class=\"mobile-header\">\r\n    <div class=\"header-item text-left\">\r\n        <div class=\"items item--hamburger\">\r\n            <a class=\"mobileMenu-toggle\" href=\"javascript:void(0)\" data-mobile-menu-toggle=\"menu\">\r\n                <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n            </a>\r\n        </div>\r\n        <div class=\"items item--searchMobile\">\r\n            <a class=\"navUser-action navUser-action--quickSearch\" href=\"javascript:void(0)\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\" aria-label=\"Search\">\r\n                <svg class=\"icon\" aria-label=\"Open Search Form\"><use xlink:href=\"#icon-search\"></use></svg>\r\n                <span class=\"icon close\" aria-label=\"Close Search\">&times;</span>\r\n            </a>\r\n        </div>\r\n    </div>\r\n    <div class=\"header-item text-center\">\r\n        <div class=\"items\">\r\n            <div class=\"logo-wrapper\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <h1 class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </h1>\r\n                {{else}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"header-item text-right\">\r\n        <div class=\"items item--account\">\r\n            <a class=\"navUser-action navUser-action--account\" href=\"javascript:void(0)\" aria-label=\"{{lang 'common.account'}}\">\r\n                <div class=\"navUser-icon\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-user\"/></svg>\r\n                </div>\r\n            </a>\r\n        </div>\r\n        <div class=\"items item--cart themevale_cart\">\r\n            <a class=\"navUser-action cartMobile\"\r\n                data-cart-preview3\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\r\n                <div class=\"navUser-icon navUser-item-cartIcon cart-icon\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                    <span class=\"countPill cart-quantity\"></span>\r\n                </div>\r\n            </a>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"
}