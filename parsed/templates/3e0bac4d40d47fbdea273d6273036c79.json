{
  "components/brand/halo-brand-toolbar": "<div class=\"halo-category-toolbar{{#if theme_settings.show_sidebar}} show-sidebar{{/if}}{{#if theme_settings.halo_sticky_header}} has-stickyHeader{{/if}}\">    \r\n    <div class=\"column-left{{#if theme_settings.show_sidebar}} sidebar-show{{else}} sidebar-hide{{/if}}\">\r\n        {{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\r\n    </div>\r\n    <div class=\"column-right\">\r\n        {{#if theme_settings.show_sidebar}}\r\n            <div class=\"sidebar-label{{#if theme_settings.show_sidebar}} show-sidebar{{/if}}\">\r\n                <span class=\"sidebar_more\">{{lang 'common.show_sidebar'}}</span>\r\n                <span class=\"icon-filter\">\r\n                    <svg class=\"icon\"><use xlink:href=\"#icon-filter\"></use></svg>\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n        {{#if brand.products}}\r\n            <form class=\"actionBar show-product\" method=\"get\" data-sort-by data-show-products>\r\n                {{> components/category/show-product limit=limit products_per_page=theme_settings.page_products_per_page}}\r\n            </form>\r\n            {{> components/products/filter sort=pagination.category.sort}}\r\n\r\n            <div class=\"view-as-btn\">\r\n                <div class=\"btn-group\">\r\n                    <a href=\"javascript:void(0);\" id=\"grid-view\" title=\"Grid View\" class=\"btn-view grid-view{{#if theme_settings.product_list_display_mode '===' 'grid'}} current-view{{/if}}\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-grid\"></use></svg>\r\n                    </a>\r\n                    <a href=\"javascript:void(0);\" id=\"list-view\" title=\"List View\" class=\"btn-view list-view{{#if theme_settings.product_list_display_mode '===' 'list'}} current-view{{/if}}\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-list\"></use></svg>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n",
  "components/common/breadcrumbs": "<nav aria-label=\"Breadcrumb\">\n    <ol class=\"breadcrumbs\" itemscope itemtype=\"http://schema.org/BreadcrumbList\">\n        {{#unless theme_settings.hide_breadcrumbs }}\n            {{#each breadcrumbs}}\n                <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"http://schema.org/ListItem\">\n                    <a class=\"breadcrumb-label\"\n                       itemprop=\"item\"\n                       href=\"{{url}}\"\n                       {{#if @last}}aria-current=\"page\"{{/if}}\n                    >\n                        <span itemprop=\"name\">{{name}}</span>\n                    </a>\n                    <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\n                </li>\n            {{/each}}\n        {{/unless}}\n    </ol>\n</nav>\n",
  "components/category/show-product": "<fieldset class=\"form-fieldset actionBar-section\">\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"limit\">{{lang 'common.number_products.show'}}</label>\r\n        <select class=\"form-select form-select--small\" name=\"limit\" id=\"limit\">\r\n            <option value=\"10\"{{#if '10' '==' products_per_page}} selected{{/if}}>10 {{lang 'common.number_products.show_text'}}</option>\r\n            <option value=\"20\"{{#if '20' '==' products_per_page}} selected{{/if}}>20 {{lang 'common.number_products.show_text'}}</option>\r\n            <option value=\"30\"{{#if '30' '==' products_per_page}} selected{{/if}}>30 {{lang 'common.number_products.show_text'}}</option>\r\n            <option value=\"40\"{{#if '40' '==' products_per_page}} selected{{/if}}>40 {{lang 'common.number_products.show_text'}}</option>\r\n            <option value=\"50\"{{#if '50' '==' products_per_page}} selected{{/if}}>50 {{lang 'common.number_products.show_text'}}</option>\r\n        </select>\r\n    </div>\r\n</fieldset>\r\n",
  "components/products/filter": "<form class=\"actionBar\" method=\"get\" data-sort-by=\"product\">\n    {{> components/category/sort-box sort=sort}}\n</form>\n",
  "components/category/sort-box": "<fieldset class=\"form-fieldset actionBar-section\">\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\n        <select class=\"form-select form-select--small \" name=\"sort\" id=\"sort\" role=\"listbox\">\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\n            {{#if result_count}}\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\n            {{/if}}\n        </select>\n    </div>\n</fieldset>\n"
}