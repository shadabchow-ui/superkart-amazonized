{
  "components/common/navigation-menu-2": "<div class=\"vertical-menu\">\r\n    <a href=\"#\" class=\"icon-nav\" aria-haspopup=\"true\">\r\n\t\t<svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-menu-grid\"/></svg>\r\n\t\t<span class=\"menu-title\">{{lang 'halothemes.categories'}}</span>\r\n\t\t<svg class=\"arrow-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\r\n\t</a>\r\n    <div class=\"vertical-menu-dropdown\" aria-label=\"Vertical Menu\">\r\n        <ul class=\"navPages-list navPages-list-depth-max{{#if theme_settings.haloMegamenu}} navPages-list-megamenu navPages-list-megamenu-2{{else}} navPages-list-depth-max{{/if}}\" data-level-list>\r\n            {{#each categories}}\r\n                {{#if id '==' ../theme_settings.left-categories-id}}\r\n                {{else}}\r\n                    <li class=\"navPages-item{{#if children}} has-dropdown{{else}} navPages-action-end{{/if}}\" data-level=\"2\">\r\n                        {{> components/common/navigation-list-alternate}}\r\n                    </li>\r\n                {{/if}}\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n</div>\r\n\r\n{{#unless theme_settings.hide_content_navigation}}\r\n\t<nav class=\"navPages\">\r\n\t    <ul class=\"navPages-list navPages-list-depth-max navPages-list-webpages\" data-level-list>\r\n\t        {{#each pages}}\r\n\t            <li class=\"navPages-item navPages-webpages-item{{#if children}} has-dropdown{{else}} navPages-action-end{{/if}}\" data-level=\"2\">\r\n\t                {{> components/common/navigation-list-alternate}}\r\n\t            </li>\r\n\t        {{/each}}\r\n\t    </ul>\r\n\t</nav>\r\n{{/unless}}\r\n\r\n<ul class=\"navPages-list navPages-list--user\" data-level-list>\r\n    <li class=\"navPages-item\">\r\n        <a class=\"navPages-action\" href=\"{{urls.account.wishlists.all}}\" aria-label=\"Wishlist\">\r\n            <span class=\"text\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-wishlist\"/></svg>{{lang 'account.mobile_nav.wishlists'}}</span>\r\n        </a>\r\n    </li>\r\n    <li class=\"navPages-item\">\r\n        <a class=\"navPages-action\" href=\"/contact-us/\" aria-label=\"Store Locator\">\r\n            <span class=\"text\"><svg class=\"icon icon-location\" aria-hidden=\"true\"><use xlink:href=\"#icon-location\"/></svg>Store Locator</span>\r\n        </a>\r\n    </li>\r\n    <li class=\"navPages-item\">\r\n        <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\" aria-label=\"Phone\">\r\n            <span class=\"text\"><svg class=\"icon icon-telephone\" aria-hidden=\"true\"><use xlink:href=\"#icon-telephone\"/></svg>Contact us: {{settings.phone_number}}</span>\r\n        </a>\r\n    </li>\r\n    {{#if currency_selector.currencies.length '>' 1}}\r\n        <li class=\"navPages-item currencies\" data-level=\"2\">\r\n            <a id=\"currency_selector\" class=\"navPages-action has-subMenu\" href=\"javascript:void(0)\">\r\n                <span class=\"text\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\r\n                <span class=\"navPages-action-moreIcon\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                </span>\r\n            </a>\r\n            <div class=\"navPage-subMenu level-2\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                <ul class=\"navPage-subMenu-list\">\r\n                    <li class=\"navPage-subMenu-item-child navPage-subMenu-title\" data-level=\"2\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"javascript:void(0)\">\r\n                            <span class=\"navPages-action-moreIcon\">\r\n                                <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                <span>Back</span>\r\n                            </span>\r\n                            <span class=\"text\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\r\n                        </a>\r\n                    </li>\r\n                    {{#each currency_selector.currencies}}\r\n                    <li class=\"navPage-subMenu-item-child navPages-action-end\" data-level=\"3\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                            <span class=\"text\">\r\n                            {{#if is_active}}\r\n                                {{{flag}}} {{name}}\r\n                            {{else}}\r\n                                {{{flag}}} {{name}}\r\n                            {{/if}}\r\n                            </span>\r\n                        </a>\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n            </div>\r\n        </li>\r\n    {{/if}}\r\n    {{#if settings.gift_certificates_enabled}}\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action\"\r\n               href=\"{{urls.gift_certificate.purchase}}\"\r\n               aria-label=\"{{lang 'common.gift_cert'}}\"\r\n            >\r\n                {{lang 'common.gift_cert'}}\r\n            </a>\r\n        </li>\r\n    {{/if}}\r\n    {{#if customer.store_credit.value '>' 0}}\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action navPages-action--storeCredit\">\r\n                {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n            </a>\r\n        </li>\r\n    {{/if}}\r\n    {{#if theme_settings.product_list_display_mode '==' 'list'}}\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action navPages-action--compare\"\r\n               href=\"{{urls.compare}}\"\r\n               data-compare-nav\r\n               aria-label=\"{{lang 'common.compare'}}\"\r\n            >\r\n                {{lang 'common.compare'}}\r\n                <span class=\"countPill countPill--positive countPill--alt\"></span>\r\n            </a>\r\n        </li>\r\n    {{/if}}\r\n\r\n    <li class=\"navPages-item\">\r\n        {{> components/common/social-links}}\r\n    </li>\r\n</ul>\r\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span class=\"text\">{{name}}</span>\n        <span class=\"navPages-action-moreIcon\">\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n            Back\n        </span>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span class=\"text\">{{name}}</span>\n    </a>\n{{/if}}\n",
  "components/common/social-links": "{{#or social_media theme_settings.social_icon_message_link theme_settings.social_icon_snapchat_link }}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#if social_media}}\n            {{#each social_media}}\n                <li class=\"socialLinks-item {{display_name}}\">\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                        <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                </li>\n            {{/each}}\n        {{/if}}\n        {{#if theme_settings.social_icon_snapchat_link '!=' ''}}\n            <li class=\"socialLinks-item snapchat\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.social_icon_snapchat_link}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg><use xlink:href=\"#icon-snapchat-ghost\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_message_link '!=' ''}}\n            <li class=\"socialLinks-item messenger\">\n                <a class=\"icon icon--messenger\" href=\"{{theme_settings.social_icon_message_link}}\" target=\"_blank\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg><use xlink:href=\"#icon-messenger\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n{{/or}}\n\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal level-2\" id=\"navPages-{{id}}\">\n    <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title navPages-action-end\" data-level=\"2\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"navPages-action-moreIcon\">\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                    Back\n                </span>\n                <span class=\"text\" href=\"{{url}}\">{{name}}</span>\n            </a>\n        </li>\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-all navPages-action-end\" data-level=\"2\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n            </a>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\" data-level=\"3\">\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu level-2\" href=\"{{url}}\">\n                        <span class=\"text\">{{name}}</span>\n                        <span class=\"navPages-action-moreIcon\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            Back\n                        </span>\n                    </a>\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal level-3\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                         <ul class=\"navPage-subMenu-list\">\n                             <li class=\"navPage-subMenu-item-child navPage-subMenu-title navPages-action-end\" data-level=\"3\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"navPages-action-moreIcon\">\n                                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                                        Back\n                                    </span>\n                                    <span class=\"text\" href=\"{{url}}\">{{name}}</span>\n                                </a>\n                            </li>\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-all navPages-action-end\" data-level=\"3\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n                                </a>\n                            </li>\n                            {{#each children}}\n                                <li class=\"navPage-subMenu-item-child\" data-level=\"4\">\n                                    <a class=\"navPage-subMenu-action navPages-action level-3\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                            {{/each}}\n                         </ul>\n                    </div>\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\" data-level=\"3\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}