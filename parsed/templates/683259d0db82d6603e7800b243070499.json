{
  "components/cart/modals/configure-product-2": "<div class=\"product-edit-header\">\r\n    <h3 style=\"-webkit-box-orient: vertical;\">\r\n    <span>{{lang 'cart.edit_product'}}</span>\r\n    <span>\"{{product.title}}\"</span>\r\n</h3>\r\n</div>\r\n<div class=\"product-edit-info scrollbar\">\r\n    <div class=\"product-edit-info-inner product-edit-wrapper\" data-count=\"0\">\r\n        <div class=\"product-edit-item product-edit-itemFirst\" data-product-edit-id=\"{{product.id}}\" data-product-edit-item-id>\r\n            <div class=\"product-edit-detail\">\r\n                <div class=\"product-edit\" data-product-edit-id=\"{{product.id}}\" data-product-edit-item-id>\r\n                    <div class=\"product-edit-image\">\r\n                        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage product.main_image 'productthumb_size'}}\" alt=\"{{product.title}}\" title=\"{{product.title}}\">\r\n                    </div>\r\n                    <div class=\"product-edit-content\">\r\n                        <div class=\"product-edit-brand\">{{product.brand.name}}</div>\r\n                        <h4 class=\"product-edit-title\">\r\n                            <span class=\"subtitle\">{{lang 'cart.this_item'}}</span>\r\n                            <span class=\"title\">{{product.title}}</span>\r\n                        </h4>\r\n                        <div class=\"product-edit-variant\"></div>\r\n                        <div class=\"cart-edit-price product-edit-price\" data-cart-edit-item-price>\r\n                            {{#if product.price.with_tax}}\r\n                                <span class=\"price\">{{product.price.with_tax.formatted}}</span>\r\n                            {{/if}}\r\n                            {{#if product.price.without_tax}}\r\n                                <span class=\"price\">{{product.price.without_tax.formatted}}</span>\r\n                            {{/if}}\r\n                        </div>\r\n                        <div class=\"cart-edit-quantity product-edit-increment\" data-cart-edit-item-quantity>\r\n                            <label class=\"form-label form-label--alternate\" for=\"qty[]\">{{lang 'products.quantity'}}</label>\r\n                            <div class=\"form-increment\" data-quantity-change>\r\n                                <button class=\"button button--icon dec\" data-cart-itemid=\"{{id}}\" data-action=\"dec\">\r\n                                    <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                                </button>\r\n                                <input class=\"form-input form-input--incrementTotal\"\r\n                                    id=\"qty-{{id}}\"\r\n                                    name=\"qty[]\"\r\n                                    type=\"tel\"\r\n                                    value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                                    data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n                                    data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n                                    data-preval=\"\"\r\n                                    min=\"1\"\r\n                                    data-cart-itemid=\"{{id}}\"\r\n                                    pattern=\"[0-9]*\"\r\n                                    aria-live=\"polite\">\r\n                                <button class=\"button button--icon inc\" data-cart-itemid=\"{{id}}\" data-action=\"inc\">\r\n                                    <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n                                </button>    \r\n                            </div>\r\n                        </div>\r\n                        <div class=\"cart-edit-remove\" data-cart-edit-item-remove>\r\n                            <a href=\"#\" data-edit-cart-remove>Ã—</a>\r\n                        </div>\r\n                        <div class=\"alertMessageBox product-edit-message\" hidden>\r\n                            {{> components/common/alert/alert-generic}}\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"product-edit-change\" data-product-option-change2 data-product-edit-option-change>\r\n                <form class=\"form\" method=\"post\" action=\"cart.php\" enctype=\"multipart/form-data\">\r\n                    <input type=\"hidden\" name=\"action\" value=\"add\">\r\n                    <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n                    <input type=\"hidden\" name=\"qty[]\" value=\"\"/>\r\n                    <div class=\"product-edit-option halo-productOptions\">\r\n                        <div class=\"cart-edit-row\" data-cart-edit-item></div>\r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<div class=\"product-edit-action\">\r\n    <div class=\"product-add-more\">\r\n        <a class=\"product-addmore-button\" href=\"#\" data-edit-cart-add-more>\r\n            <span class=\"addmore-icon\">+</span>\r\n            <span class=\"addmore-text\">Add more</span>\r\n        </a>\r\n    </div>\r\n    <div class=\"product-edit-button\">\r\n        <button class=\"button button--large button--primary\" data-edit-cart-add-to-cart data-wait-message=\"{{lang 'products.adding_to_cart'}}\">Add to Cart</button>\r\n    </div>\r\n</div>\r\n",
  "components/common/alert/alert-generic": "<div class=\"alertBox\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n"
}