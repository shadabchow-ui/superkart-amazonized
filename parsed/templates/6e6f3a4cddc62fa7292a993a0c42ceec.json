{
  "components/common/navigation-menu": "<nav class=\"navPages\" role=\"navigation\" aria-label=\"Main Navigation\">\n    <ul id=\"navPages-list-main\" class=\"navPages-list navPages-list-depth-max{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{else}} navPages-list-depth-max{{/if}}\" data-level-list>\n        {{#each categories}}\n            <li class=\"navPages-item navPages-item-show{{#if children}} has-dropdown background-hover{{else}} navPages-action-end{{/if}}\" data-level=\"2\">\n                {{> components/common/navigation-list-alternate}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                 <li class=\"navPages-item navPages-item-page navPages-item-show{{#if children}} has-dropdown background-hover{{else}} navPages-action-end{{/if}}\" data-level=\"2\">\n                     {{> components/common/navigation-list-alternate}}\n                 </li>\n             {{/each}}\n        {{/unless}}\n        <li class=\"navPages-item navPages-item-custom u-hiddenVisually has-subMenu background-hover\">\n            <a class=\"navPages-action has-subMenu is-root\" href=\"javascript:void(0)\" aria-label=\"Custom Menu\">\n                <span class=\"text\" >...</span>\n            </a>\n            <div class=\"navPage-subMenu navPage-subMenu-level1\" id=\"navPages-last\" aria-hidden=\"true\" tabindex=\"-1\">\n                <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n                        \n                </ul>\n            </div>\n        </li>\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\" data-level-list>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.account.wishlists.all}}\" aria-label=\"Wishlist\">\n                <span class=\"text\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-wishlist\"/></svg>{{lang 'account.mobile_nav.wishlists'}}</span>\n            </a>\n        </li>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"/contact-us/\" aria-label=\"Store Locator\">\n                <span class=\"text\"><svg class=\"icon icon-location\" aria-hidden=\"true\"><use xlink:href=\"#icon-location\"/></svg>Store Locator</span>\n            </a>\n        </li>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\" aria-label=\"Phone\">\n                <span class=\"text\"><svg class=\"icon icon-telephone\" aria-hidden=\"true\"><use xlink:href=\"#icon-telephone\"/></svg>Contact us: {{settings.phone_number}}</span>\n            </a>\n        </li>\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item currencies\" data-level=\"2\">\n                <a class=\"navPages-action has-subMenu\"\n                   href=\"#\"\n                   data-collapsible=\"navPages-currency\"\n                   aria-controls=\"navPages-currency\"\n                   aria-expanded=\"false\"\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\n                >\n                    <span class=\"image-flag\">{{{currency_selector.active_currency_flag}}}</span>\n                    <span class=\"text-flag\" style=\"-webkit-box-orient: vertical;\">{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use>\n                        </svg>\n                    </i>\n                </a>\n                <div class=\"navPage-subMenu level-2\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\" data-level=\"2\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"javascript:void(0)\">\n                                <span class=\"navPages-action-moreIcon\">\n                                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                                    <span>Back</span>\n                                </span>\n                                <span class=\"text\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\n                            </a>\n                        </li>\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\" data-level=\"3\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{{switch_url}}}\"\n                               aria-label=\"{{name}}\"\n                               data-currency-code=\"{{{code}}}\"\n                               {{#if cart_currency_switch_url.length '>' 0 }}\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                               {{/if}}\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                            >\n                                <span class=\"text\">\n                                    {{#if is_active}}\n                                        {{{flag}}} {{name}}\n                                    {{else}}\n                                        {{{flag}}} {{name}}\n                                    {{/if}}\n                                </span>\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >   \n                    <span class=\"text\">\n                        <svg class=\"icon\"><use xlink:href=\"#icon-gift-card\"/></svg>\n                        {{lang 'common.gift_cert'}}\n                    </span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\"\n                   href=\"{{urls.compare}}\"\n                   data-compare-nav\n                   aria-label=\"{{lang 'common.compare'}}\"\n                >\n                    {{lang 'common.compare'}}\n                    <span class=\"countPill countPill--positive countPill--alt\"></span>\n                </a>\n            </li>\n        {{/if}}\n\n        <li class=\"navPages-item\">\n            {{> components/common/social-links}}\n        </li>\n    </ul>\n</nav>\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span class=\"text\">{{name}}</span>\n        <span class=\"navPages-action-moreIcon\">\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n            Back\n        </span>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span class=\"text\">{{name}}</span>\n    </a>\n{{/if}}\n",
  "components/common/social-links": "{{#or social_media theme_settings.social_icon_message_link theme_settings.social_icon_snapchat_link }}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#if social_media}}\n            {{#each social_media}}\n                <li class=\"socialLinks-item {{display_name}}\">\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                        <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                </li>\n            {{/each}}\n        {{/if}}\n        {{#if theme_settings.social_icon_snapchat_link '!=' ''}}\n            <li class=\"socialLinks-item snapchat\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.social_icon_snapchat_link}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg><use xlink:href=\"#icon-snapchat-ghost\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_message_link '!=' ''}}\n            <li class=\"socialLinks-item messenger\">\n                <a class=\"icon icon--messenger\" href=\"{{theme_settings.social_icon_message_link}}\" target=\"_blank\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg><use xlink:href=\"#icon-messenger\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n{{/or}}\n\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal level-2\" id=\"navPages-{{id}}\">\n    <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title navPages-action-end\" data-level=\"2\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"navPages-action-moreIcon\">\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                    Back\n                </span>\n                <span class=\"text\" href=\"{{url}}\">{{name}}</span>\n            </a>\n        </li>\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-all navPages-action-end\" data-level=\"2\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n            </a>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\" data-level=\"3\">\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu level-2\" href=\"{{url}}\">\n                        <span class=\"text\">{{name}}</span>\n                        <span class=\"navPages-action-moreIcon\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            Back\n                        </span>\n                    </a>\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal level-3\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                         <ul class=\"navPage-subMenu-list\">\n                             <li class=\"navPage-subMenu-item-child navPage-subMenu-title navPages-action-end\" data-level=\"3\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"navPages-action-moreIcon\">\n                                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                                        Back\n                                    </span>\n                                    <span class=\"text\" href=\"{{url}}\">{{name}}</span>\n                                </a>\n                            </li>\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-all navPages-action-end\" data-level=\"3\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n                                </a>\n                            </li>\n                            {{#each children}}\n                                <li class=\"navPage-subMenu-item-child\" data-level=\"4\">\n                                    <a class=\"navPage-subMenu-action navPages-action level-3\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                            {{/each}}\n                         </ul>\n                    </div>\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\" data-level=\"3\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}